#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	//code
	int test;
	cin>>test;
	while(test--)
	{
	    int size1,size2,flag=1;
	    cin>>size1>>size2;
	    int arr1[size1],arr2[size2];
	    for(int i=0;i<size1;i++)
	        cin>>arr1[i];
	    for(int i=0;i<size2;i++)
	       cin>>arr2[i];     
	    set<int> s;
	    for(int i=0;i<size1;i++)
	     s.insert(arr1[i]);
	    for(int i=0;i<size2;i++) 
	    {
	        if(s.find(arr2[i])==s.end())
	          {flag=0;
	          cout<<"No"<<endl;
	          break;}
	        
	    }
	    if(flag==1)
	    cout<<"Yes"<<endl;
	    
	}
	return 0;
}
