#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	//code
	int test;
	cin>>test;
	while(test--)
	{
	    int size,sum,count=0;
	    cin>>size>>sum;
	    int arr[size];
	    for(int i=0;i<size;i++)
	        cin>>arr[i];
	   for(int i=0;i<size-1;i++)
	    for(int j=i+1;j<size;j++)
	     if(arr[i]+arr[j]==sum)
	        count++;
	   cout<<count<<endl;     
	   }
	  
	   
	
	return 0;
}
